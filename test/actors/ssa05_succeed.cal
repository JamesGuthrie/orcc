actor ssa05() bool Port1 ==> int Port2, int Port3:
  
  int res;
  
  procedure my_proc()
  var
    int i := 0, int j := 0, int k := 0
  begin
    while i < 10 do
      k := k + i;
      while j < 10 do
        j := j + 1;
      end
      k := k + j * 2;
      i := i + 1;
    end
    k := k + 1;
    res := k * 3;
  end

  action ==> Port2:[res]
  do
    my_proc ();
  end

end