actor expr01() int inp ==> int outp:
  
  int acc := 0;
  
  function f(int i) --> int :
    i *
      if i < 0 then
        0
      else
        if i > 255 then
          255
        else
          i
        end
      end
  end
  
  action inp:[i] ==> outp:[f(i)]
  do
    acc := acc + f(lshift(i, 1)) + f(f(i));
  end

end