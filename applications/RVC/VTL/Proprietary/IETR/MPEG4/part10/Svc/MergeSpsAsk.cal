package Proprietary.IETR.MPEG4.part10.Svc;

actor MergeSpsAsk()
	uint(size=5) SpsAskedFromPic,
	uint(size=5) SpsAskedFromPps,
	bool         SpsAskedFromSpsSvc
		==>
	uint(size=5) SpsAsked,
	bool         IsSizeDPB
		:

	uint(size=2) GET_SPS_FROM_PIC = 0;
	uint(size=2) GET_SPS_FROM_PPS = 1;
	uint(size=2) GET_SPS_FROM_SVC = 2;

	GetSpsForPic: action
			SpsAskedFromPic :[NumSps]
				==>
			SpsAsked        :[GET_SPS_FROM_PIC , NumSps],
			IsSizeDPB       :[true]
		end

	GetSpsForSvc: action
			SpsAskedFromSpsSvc :[sps_asked]
				==>
			SpsAsked           :[GET_SPS_FROM_SVC],
			IsSizeDPB          :[false]
		end

	GetSpsForPps: action
			SpsAskedFromPps :[NumSps]
				==>
			SpsAsked        :[GET_SPS_FROM_PPS , NumSps]
		end
end