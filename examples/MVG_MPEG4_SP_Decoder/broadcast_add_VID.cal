actor broadcast_add_VID (
	int PIX_SZ
)

  int(size=PIX_SZ) VIDIN ==> int(size=PIX_SZ) VIDOUT1, int(size=PIX_SZ) VIDOUT2:

  int(size=PIX_SZ) o1 := 0;
  int(size=PIX_SZ) o2 := 0;
  int(size=PIX_SZ) i := 0;

  action VIDIN:[i] ==> VIDOUT1:[o1], VIDOUT2:[o2]
  do
    o1 := i;
    o2 := i;
  end

end